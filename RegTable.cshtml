@{ 
    Layout = null;
}
@model List<Network.Models.UserModel>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    @*<title>@ViewData["Title"] - Network</title>*@
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.js"></script>



</head>
<body>

    <nav class="navbar navbar-expand navbar-toggleable-sm navbar-light bg-dark border-bottom box-shadow mb-3 navbar-fixed-top margin-left:14 ">
        <div class="container">
            @*<a class="navbar-brand" asp-area="" asp-controller="Network" asp-action="Index">Network</a>*@
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between col-12">
                <ul class="navbar-nav flex-grow-1" style="margin-left:-60px;">
                    <li class="nav-item text-dark text-white">
                        <a class="btn btn-danger" style="margin-left: -51px;">DashBoard:-</a>

                    </li>
                    <li class="nav-item text-white text-left">
                        <a class="btn btn-primary">@ViewBag.Name</a>

                    </li>
                </ul>
                <ul class="nav navbar-nav navbar-collapse">
                    <li class="text-white">
                        <h4>Welcome to User Dashboard</h4>

                    </li>
                </ul>

                <ul class="nav navbar-nav navbar-right" style="margin-right:-100px;">
                    <li><a asp-controller="Network" asp-action="Logout" class="btn btn-danger">Logout</a></li>

                </ul>
            </div>
        </div>
    </nav>

    <div class="row">

        <div class="col-md-2" style="margin-top:-12px;">

            <ul class="nav flex-column  bg-dark text-white ">

                <li class="nav-item">
                    <div form-row>

                        <div class="col-2">

                            <img class="rounded-circle " src="~/@ViewBag.image" height="50px;" width="50px" data-holder-rendered="true" style="margin-top: 5px;">
                            <div>
                                <h6 style=" margin-left: 59px; margin-top: -34px;">@ViewBag.Name</h6>
                            </div>
                            <br />


                            <div>
                                <a asp-controller="Network" asp-action="Edit" class="text-white" style=" margin-left: 10px;">UpdateProfile</a>
                            </div>
                        </div>
                    </div>


                </li>
                <br />

                <li class="nav-item">

                    <a value="@ViewBag.Crbalance" class="text-white" style="margin-left: 25px;">Withdrawal</a>

                </li>
                <br />

                <li class="nav-item">
                    <a asp-controller="Network" asp-action="RegTable1" class="text-white" style="margin-left: 25px;">UserTable</a>
                </li>
                <br />

                <li class="nav-item">
                    <a asp-controller="Network" asp-action="Registration" class="text-white" style="margin-left: 25px;">AddUser</a>
                </li>
                <br />

                <li class="nav-item">
                    <a asp-controller="Network" asp-action="Image" class="text-white" style="margin-left: 25px;">Earning_Level</a>
                </li>
                <br />

                <li class="nav-item">
                    <a asp-controller="Network" asp-action="Privacy" class="text-white" style="margin-left: 25px;">Help</a>
                </li>
                <br />
                <br />
                <br />
                <br />
                <br />
                <br />
                <br />
                <br />
                <br />
                <br />
                <br />
                <br />
                <br />

            </ul>
        </div>


        <div class="col-md-10">

            <div>
                <label class="col-md-2"> <h5>Wallet Amount:-</h5> </label>
                <input type="number" disabled="disabled" value="@ViewBag.sal" class="col-lg-1" />
               
                <label class="col-md-2"> <h5>TotalUsers:-</h5> </label>
                <input type="number" disabled="disabled" value="@ViewBag.re" class="col-lg-1" />
                <label class="col-md-1"> <h5>Level:-</h5> </label>
                <input type="number" disabled="disabled" value="@ViewBag.level" class="col-lg-1" />
                <label class="col-md-1"> <h5>Bones:-</h5> </label>
                <input type="number" disabled="disabled" value="@ViewBag.bonus" class="col-lg-1" />
            </div>
            <div>
                <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                    <ol class="carousel-indicators">
                        <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                        <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                        <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                    </ol>
                    <div class="carousel-inner">
                        <div class="carousel-item active">
                            <img class="d-block w-100" src="~/15.png" alt="First slide">
                        </div>
                        <div class="carousel-item">

                            <img src="~/16.png" />
                        </div>
                        <div class="carousel-item">
                            <img class="d-block w-100" src="~/17.png" alt="Third slide">
                        </div>
                    </div>
                    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                        <span class="sr-only">Previous</span>
                    </a>
                    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                        <span class="sr-only">Next</span>
                    </a>
                </div>
            </div>

        </div>
    </div>
    <hr style="height:2px;border-width:0;color:black;background-color:black; margin-top:-1px;">
  

    <div class="container">
        <main role="main" class="pb-3">
            @*@RenderBody()*@
        </main>
    </div>

    @*<footer class="border-top footer text-muted">
            <div class="container">
                &copy; 2022 - Network - <a asp-area="" asp-controller="Network" asp-action="Privacy">Privacy</a>
            </div>
        </footer>*@
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/table.js"></script>
    @*@await RenderSectionAsync("Scripts", required: false)*@
</body>

